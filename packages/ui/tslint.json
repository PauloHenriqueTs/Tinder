{
  "extends": ["../../tslint.json", "tslint-react"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  },
  "rules": {
    "jsx-no-multiline-js": false,
    "jsx-curly-spacing": false,
    "jsx-no-lambda": false,
    "no-shadowed-variable": false,
    "no-implicit-dependencies": false
  }
}
